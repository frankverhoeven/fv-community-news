	<AkismetEnabled type="bool">false</AkismetEnabled>
	<LoggedIn type="bool">false</LoggedIn>
	<RssEnabled type="bool">true</RssEnabled>
	<CaptchaEnabled type="bool">false</CaptchaEnabled>
	<HideCaptchaLoggedIn type="bool">true</HideCaptchaLoggedIn>
</config>
