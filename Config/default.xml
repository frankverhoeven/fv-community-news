<?xml version="1.0" encoding="UTF-8"?>

<config>
	<Version type="string">1.4</Version>
	<DbName type="string">fv_community_news</DbName>
	<DbVersion type="string">2.0</DbVersion>
	<SubmissionTemplate type="string"><![CDATA[<li>
	<h4><a href="%submission_url%">%submission_title%</a></h4>
	<small>%submission_date%</small>
	%submission_description%
</li>]]></SubmissionTemplate>
